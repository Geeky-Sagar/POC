                                                    <div class="item-box-blog-data" style="padding: px 15px;">
                                                        <p><i class="fa fa-user-o"></i>Features</p>
                                                    </div>
                                                    <!--Text-->
                                                    <div class="item-box-blog-text">
                                                        <p>feature 1</p>
                                                        <p>feature 2</p>
                                                        <p>feature 3</p>
                                                    </div>
                                                    <div class="mt">
                                                        <input type="button" class="btn bg-blue-ui white read btnCheckout" value="Checkout" id="buttonId3" data-subscription-type="pro" />
